@page
@model Veeggan.Pages.Admin.FoodTypes.DeleteModel


<form method="post">
    <input hidden asp-for="FoodType.Id" />
    <div>
        <div class="border p-3 mt-4">
            <div class="row pb-2">
                <h2 class="text-primary pl-3">Delete FoodType</h2>
                <hr/>
             </div>

            <div class="mb-3">
                <label asp-for="FoodType.Name" ></label>
                <input asp-for="FoodType.Name" disabled class="form-control"/>
            </div>
           

            <button type="submit" class="btn btn-danger" style="width:150px;">Delete</button>
            <a  asp-page="Index" class="btn btn-info" style="width:150px;">Back To List</a>
        </div>

    </div>
   
</form>


@section Scripts{
    @{
    <partial name="_ValidationScriptsPartial"/>
    }
}